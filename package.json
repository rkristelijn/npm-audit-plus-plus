{
  "name": "npm-audit-plus-plus",
  "version": "1.0.4",
  "description": "A tool to capture the output of npm audit and convert it to xml",
  "main": "src/cli.cjs",
  "bin": {
    "npm-audit-plus-plus": "src/cli.cjs"
  },
  "scripts": {
    "test": "cat fixtures/audit.json | node src/cli.cjs",
    "test-ok": "cat fixtures/audit-ok.json | node src/cli.cjs",
    "test-critical": "cat fixtures/audit-criticals.json | node src/cli.cjs",
    "start": "node src/cli.cjs",
    "build": "babel src/cli.cjs --out-file dist/cli.js"
  },
  "keywords": [
    "npm",
    "audit",
    "xml",
    "gitlab",
    "pipeline",
    "security",
    "dependency management",
    "ignore",
    "vulnerabilities",
    "continuous integration"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.1.0",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rkristelijn/npm-audit-plus-plus"
  }
}
